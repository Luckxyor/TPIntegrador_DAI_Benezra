{
  "expo": {
    "name": "TP Integrador",
    "slug": "tpintegrador-mobile",
    "scheme": "tpintegrador",
    "version": "1.0.0",
    "orientation": "portrait",
    
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": ""
    },
    "plugins": [
      ["expo-router"],
      ["expo-secure-store"]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "local"
      }
    }
  }
}


